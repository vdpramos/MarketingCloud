    <style>
        .centerwidth {
            width: 600px;
        }

        @media screen and (max-width: 600px) {
            .full-width {
                width: 100% !important;
            }
        }

        .tabelasorteio {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }

        .tabelasorteio th {
            font-size: 15px;
            line-height: 20px;
            color: #475EFF;
            white-space: nowrap;
        }

        .tabelasorteio td,
        th {
            border: 1px solid #060606;
            text-align: center;
            padding: 8px;
            white-space: normal
        }

        .tabelasorteio tr:nth-child(even) {
            background-color: #dddddd;
        }

        .styled-table {
            border-collapse: collapse;
            margin: 25px 0;
            font-size: 0.9em;
            font-family: sans-serif;
            word-wrap:break-word;
            min-width: 400px;
            width: 100%;
            box-shadow: 0 0 20px rgba(0, 0, 0, 0.15);
        }

        .styled-table tbody tr {
            border-bottom: 1px solid #dddddd;
        }
        
        .styled-table tbody tr:nth-of-type(even) {
            background-color: #f3f3f3;
        }
        
        .styled-table tbody tr:last-of-type {
            border-bottom: 2px solid #009879;
        }

        .styled-table thead tr {
            background-color: #475EFF;
            color: #ffffff;
            text-align: center;
        }

        .styled-table th,
.styled-table td {
    padding: 13px 13px;
    word-wrap:break-word;
    white-space: nowrap;
    border: 1px solid #060606;
}

        .styled-table tbody tr.active-row {
            font-weight: bold;
            color: #009879;
        }

        .watermark {
                        
             text-align: left; font-size: 11px; color: #324B68; font-family: arial; font-size: 11px;
                                       
        }
    </style>


</head>


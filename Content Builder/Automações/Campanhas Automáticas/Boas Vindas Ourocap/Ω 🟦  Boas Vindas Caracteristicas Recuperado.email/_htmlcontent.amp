                                <tbody>
                                    %%[ 

                                    var @rows, @row, @rowCount, @origem, @counter, @nome
                                    
                                    set @rows = LookupRows('TB_BOAS_VINDAS_OUROCACAP_CARACTERISTICAS_TITULOS', 'CPF_CNPJ', CPF_CNPJ)
                                    
                                    set @rowCount = rowcount(@rows) if @rowCount > 0 then for @counter = 1 to @rowCount do 
                                    set @row = row(@rows, @counter) 
                                    
                                    /* set @NUMERO_DO_TITULO = field(@row, 'HC_NUMERO_DO_TITULO__C') */
                                   set @Nome = field(@row, 'Nome_produto') 
set @Nome = ProperCase(@Nome)
                                    set @NUMERO_DA_SORTE = field(@row, 'NUMERO_DA_SORTE')
                                    set @VALOR_DA_PARCELA = FormatNumber (field(@row, 'VALOR_PARCELA'), "F0")
                                    /* set @TERMINO_CARENCIA = field(@row, 'HC_Termino_Carencia__c') */
                                    set @FIM_PREVISTO = field(@row, 'FIM_PREVISTO')
                                    set @INICIO = field(@row, 'INICIO')
                                    set @Nome_Produto = field(@row, 'NOME_PRODUTO')
                                    set @Nome_Produto = ProperCase(@Nome_Produto)
                                    
                                    ]%% 
                                    
                                    <tr class="active-row">
                                    <!-- <tr class="active-row"> -->
                                        <td style="font-size: 14px;">%%=v(@NOME_PRODUTO)=%%</td>
                                        <td>%%=DateDiff(@INICIO,@FIM_PREVISTO, 'M')=%% meses</td>
                                        <td style="
                                        font-size: 14px; text-align: center;
                                        white-space: nowrap;
                                    ">R$ %%=v(@VALOR_DA_PARCELA)=%%</td>
                                    
                                          <td style="
                                        font-size: 14px; text-align: center;
                                        white-space: nowrap;
                                    ">%%=v(@NUMERO_DA_SORTE)=%%</td>
                                         
                                    </tr>
                                    <!-- and so on... -->
                                    %%[ next ]%%

    
                                %%[ endif ]%%
